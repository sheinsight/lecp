{
	"$schema": "./node_modules/oxfmt/configuration_schema.json",
	"useTabs": true,
	"arrowParens": "avoid",
	"printWidth": 80,
	"ignorePatterns": [
		"**/crates/**/npm/**",
		"**/crates/node_binding/index.*",
		"**/target/**",
		"**/dist/**",
		"**/es/**",
		"**/lib/**",
		"**/umd/**",
		"**/tests/**/snapshots/**",
		"**/doc_build/**",
		"**/compiled/**"
	]
}
